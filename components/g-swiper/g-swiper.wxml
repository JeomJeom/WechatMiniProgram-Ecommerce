<!--components/w-swiper/g-swiper.wxml-->
<swiper class='swiper' 
        circular 
        autoplay 
        interval='3000' 
        duration='300' 
        indicator-dots 
        indicator-active-color='#FF7F50'>
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"/>
    </swiper-item>  
  </block>
</swiper>
